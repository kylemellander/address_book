<h2><%= @new_contact.title() %> <%= @new_contact.first_name() %> <%= @new_contact.last_name() %></h2>
<h4><%= @new_contact.job_title() %></h4>

<div class="row jk-phones">
  <h3>Phone Numbers:</h3>

  <ul>
    <% Phone.find_by_contact_id(@new_contact.id()).each() do |phone| %>
    <li>
      <%= phone.phone_type() %> - <%= phone.number() %>
    </li>
    <% end %>
  </ul>
</div>

<div class="row jk-addresses">
  <h3>Addresses:</h3>
</div>

<div class="row jk-emails">
  <h3>Email Addresses:</h3>
</div>
